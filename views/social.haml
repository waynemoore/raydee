.social

  .img-stream
    %h3 Image Stream
    - @social_widgets[:images].each do |widget|
      = haml widget[:type], :locals => { widget[:type] => widget }

  .text-stream
    %h3 Text Stream
    - @social_widgets[:text].each do |widget|
      = haml widget[:type], :locals => { widget[:type] => widget }
